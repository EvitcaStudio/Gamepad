{
  "entryPoints": ["src/index.ts"],
  "out": "docs",
  "name": "Gamepad",
  "readme": "README.md",
  "theme": "default",
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeExternals": true,
  "includeVersion": true,
  "sort": ["source-order"],
  "kindSortOrder": [
    "Project",
    "Module",
    "Namespace",
    "Enum",
    "Class",
    "Interface",
    "Constructor",
    "Property",
    "Variable",
    "Function",
    "Accessor",
    "Method"
  ],
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "searchInComments": true,
  "cleanOutputDir": true,
  "githubPages": true,
  "gitRevision": "main",
  "excludeNotDocumented": false
}
